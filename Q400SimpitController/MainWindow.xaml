<Window x:Class="Q400SimpitController.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Q400SimpitController"
        mc:Ignorable="d"
        Title="Q400 Simpit Controller" Height="600" Width="800" d:DataContext="{d:DesignInstance}">
    <Grid>
        <TabControl>
            <TabItem Header="Setup">
                <local:SetupForm Setup="{Binding Setup}" SaveSetupEvent="SetupFormOnSaveSetup"/>
            </TabItem>
            <TabItem Header="Mappings" />
        </TabControl>
    </Grid>
</Window>
